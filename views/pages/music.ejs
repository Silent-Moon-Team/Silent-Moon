<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>music</title>
</head>
<body>
    <h1>music page</h1>
    <% data.forEach(element=> { %>

        <a href=<%=`/med_player/${data.indexOf(element)}`%>>


            <h2>
                <%= element.track.name %>
            </h2>
            <audio controls>
                <source src=<%=element.track.preview_url %>>
            </audio>

        </a>

        <% }) %>
</body>
</html>